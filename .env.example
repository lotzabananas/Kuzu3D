# Copy this file to .env.local and add your actual API keys
# Never commit .env.local to version control!

# OpenAI API Configuration
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_ORG_ID=org-your-org-id-here-optional

# Optional: Set specific model versions
WHISPER_MODEL=whisper-1
GPT_MODEL=gpt-4-turbo-preview

# Server Configuration
NODE_ENV=development
PORT=3000

# Feature Flags
ENABLE_VOICE=true
ENABLE_AI_CYPHER=true

# AWS Amplify Configuration
VITE_AWS_REGION=us-east-1
VITE_USER_POOL_ID=
VITE_USER_POOL_CLIENT_ID=
VITE_IDENTITY_POOL_ID=
VITE_OAUTH_DOMAIN=
VITE_REDIRECT_SIGN_IN=https://localhost:8081
VITE_REDIRECT_SIGN_OUT=https://localhost:8081
VITE_API_ENDPOINT=http://localhost:3000
VITE_S3_BUCKET=

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
VITE_STRIPE_PRO_PRICE_ID=
VITE_STRIPE_ENTERPRISE_PRICE_ID=

# Additional Feature Flags
VITE_ENABLE_AUTH=false
VITE_ENABLE_PAYMENTS=false
VITE_ENABLE_COLLAB=false

# Database Configuration
DB_PATH=/Users/timmac/Desktop/Kuzu3D/test-kuzu-db